.\" -----------------------------------------------------------------
.\" RESUME MACROS
.\" Author: Rob Bocchino
.\" -----------------------------------------------------------------
.\" depend on ms
.mso ms.tmac 
.\" -----------------------------------------------------------------
.\" Margins
.\" -----------------------------------------------------------------
.po 1i
.ll 6.5i
.nr PO 1i
.nr LL 6.5i
.nr HM 1i
.nr FM 1i
.\" -----------------------------------------------------------------
.\" Name
.\" -----------------------------------------------------------------
.de NA
.  ft B
.  vs +3p
.  ps +3p
.  ce 1
\\$1
.  ft
.  vs
.  ps
.  sp 1.0v
..
.\" -----------------------------------------------------------------
.\" Flush left
.\" -----------------------------------------------------------------
.de FL
.  br
\\$1
..
.\" -----------------------------------------------------------------
.\" Flush right
.\" -----------------------------------------------------------------
.de FR
.  br
.  ad r
\\$1
..
.\" -----------------------------------------------------------------
.\" Subject heading
.\" -----------------------------------------------------------------
.de SU
.  ad l
.  SH 1
.  vs +1p
\s+1\\$1\s0
.  vs
.  sp -0.2v
\D'l \\n(.lu 0'
.  sp -0.3v
.  ft R
..
.\" -----------------------------------------------------------------
.\" Institution
.\" -----------------------------------------------------------------
.de IN
.  sp 0.5v
.  ft B
.  nf
.  ta \\n(.luR
\\$1	\\$2
.  ft R 
.  sp -0.3v
..
.\" -----------------------------------------------------------------
.\" Italicized heading
.\" -----------------------------------------------------------------
.de IH
.  sp -0.3v
.  SH 1
\fI\\$1\fR
..
.\" -----------------------------------------------------------------
.\" Paragraph
.\" -----------------------------------------------------------------
.de PA
.  LP
.  sp 0.2v
.  ad b
.  fi
..
.\" -----------------------------------------------------------------
.\" Paragraph with header
.\" -----------------------------------------------------------------
.de PH
.  PA
\fI\\$1\fR.
..
.\" -----------------------------------------------------------------
.\" Citation
.\" -----------------------------------------------------------------
.de CT
.  ad b
.  fi
\\$1, \fI\\$2\fR.  \\$3.
..
